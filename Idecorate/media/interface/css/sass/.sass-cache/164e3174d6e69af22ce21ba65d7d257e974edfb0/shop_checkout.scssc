	    }

	    input {
	        width: 350px;
	    }

	    select {
	        width: 350px;
	    }
	}

	.paymentMethod2 {

	    div {
	        margin-bottom: 15px;
	        
	        input {
	            float: left;
	            margin-top: 5px;
	        }

	        label {
	            display: block;
	            width: 110px;
	            height: auto;
	            padding-top: 8px;
	            float: left;
	        }

	        img {
	            float: left;
	        }

	    }

	}

}

.leftPannel {
	margin-right: 20px;
}

.disclaimer{
