:@template"2#content {
	padding: 15px;
	background: #FFFFFF;

	h3{ 
		width: 100%;
		height: 20px;
		margin: 0 0 15px 0;
		padding: 0 0 15px 0;
		border-bottom: 2px solid #000000;
		color: #000000;
	}

}

.checkout{
	background: #FFFFFF;

	input, select {
	  border: 1px solid #CCC;
	  padding: 5px;
	  -moz-border-radius: 3px;
	  -webkit-border-radius: 3px;
	  border-radius: 3px;
	  -khtml-border-radius: 3px;
	}

	#edit_button {
		padding: 4px 8px;
		cursor: pointer;
		filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#FFFFFF', endColorstr='#da9b94'); /* for IE */
      background: -webkit-gradient(linear, left top, left bottom, from(#FFFFFF), to(#da9b94)); /* for webkit browsers */
      background: -moz-linear-gradient(top,  #FFFFFF,  #da9b94); /* for firefox 3.6+ */
      background: -o-linear-gradient(top,  #FFFFFF,  #da9b94);
		-moz-border-radius: 4px;
		-webkit-border-radius: 4px;
		border-radius: 4px; /* future proofing */
		-khtml-border-radius: 4px; /* for old Konqueror browsers */
		border: 0;
	}
}

.orderWrap {


	.errorBlock {
		color: #B94A48;
		padding: 8px 35px 8px 14px;
		margin: 0 auto 15px auto;
		background: #F2DEDE;
		border: 1px solid #EED3D7;
		-webkit-border-radius: 4px;
		-moz-border-radius: 4px;
		border-radius: 4px;
		position: relative;

		.closeX {
			color: #000000;
			line-height: 18px;
			text-decoration: none;
			opacity: 0.2;
			filter:alpha(opacity=20);
			float: right;
			position: relative;
			top: -2px;
			right: -21px;
		}

		h4 {
			font-size: 14px;
			font-weight: bold;
			line-height: 18px;
		}

		ol {
			padding-left: 30px;

			li {
				display: list-item;
				list-style: decimal;
				line-height: 18px;
				text-align: -webkit-match-parent;
			}
		}
	}


	fieldset {
		border: 1px solid #000000;
		padding: 10px;

		legend{
			margin: 0 0 0 20px;
			padding: 0 5px;
			font-size: 14px;
			color: #000000;
		}

		table{
			width: 100%;
			height: auto;

			thead{

				tr{
					th{
						padding: 10px;
						color: #000000;
						border-bottom: 2px dotted #999999;
					}

				}

			}

			tbody{
				tr{
					td{
						padding: 10px;
						margin: 0 auto;

						img{
							max-width: 50px;
							max-height: 80px;
							float: left;
							border: 2px solid #999999;
						}

						span{
							width: 400px;
							height: auto;
							display: block;
							margin: 20px 0 0 60px;
							font-size: 14px;
							font-weight: bold;
							color: #d89b96;
						}

					}

					.productPricing {
						text-align:center;
						color: #000000;
					}

				}

			}

			tfoot{

				tr{
					td{
						padding: 10px;
						color: #000000;
						border-top: 2px dotted #999999;
					}
				}
			}

		}

	}
	
	
	
}

.loginDetails{
	width: auto;
	height: auto;
	margin: 15px auto;
	padding: 10px;
	background: #fbf6f0;
	box-shadow: 3px 3px 3px 2px #EEEEEE;

	h4{
		font-weight: bold;
		margin: 0 0 15px 0;
		color: #000000;
	}

	p {
		margin: 10px auto;
	}
	
	input{
		width: 250px;
		margin: 0 15px 0 0;
	}

	.submit{
		width: 80px;
		padding: 3px;
		cursor: pointer;
		filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#FFFFFF', endColorstr='#da9b94'); /* for IE */
      background: -webkit-gradient(linear, left top, left bottom, from(#FFFFFF), to(#da9b94)); /* for webkit browsers */
      background: -moz-linear-gradient(top,  #FFFFFF,  #da9b94); /* for firefox 3.6+ */
      background: -o-linear-gradient(top,  #FFFFFF,  #da9b94);
		-moz-border-radius: 4px;
		-webkit-border-radius: 4px;
		border-radius: 4px; /* future proofing */
		-khtml-border-radius: 4px; /* for old Konqueror browsers */
		border: 0;
	}

	a {
		color: #d89b96;
	}

}

.information{
    width: 100%;
    height: auto;
    margin: 0 0 15px 0;

    fieldset{
        width: 449px;
        border: 1px solid #000000;
        float: left !important;
        padding: 10px;

        legend{
            margin: 0 0 0 20px;
            padding: 0 5px;
            font-size: 14px;
        }

    }

    .personal{
        margin: 0 20px 0 0;

        div {
            margin-bottom: 15px;
        
            label {
                display: block;
                width: 70px;
                height: auto;
                padding-top: 10px;
                float: left;
            }

            input {
                width: 350px;
            }

        }

    }

    .delivery{

        div {
            margin-bottom: 15px;
        
            label {
                display: block;
                width: 70px;
                height: auto;
                padding-top: 10px;
                float: left;
            }

            input {
                width: 350px;
            }

            select {
                width: 365px;
            }

        }
        
    }

}

