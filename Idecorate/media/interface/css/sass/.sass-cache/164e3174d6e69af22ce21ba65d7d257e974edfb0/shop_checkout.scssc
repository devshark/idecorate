        background: -moz-linear-gradient(top,  #FFC5C3,  #C7544E); /* for firefox 3.6+ */
    }

	ul{
        margin: 10px 0 0 30px;
		list-style-position: inside;

        li{
            list-style-type: disc;
            margin: 0 0 10px 15px;

            a {
                color: #d89b96;
            }

        }

    }

    ol {
    	margin: 10px 0 0 30px;
		list-style-position: inside;

        li {
            list-style-type: decimal;
            margin: 0 0 10px 45px;

